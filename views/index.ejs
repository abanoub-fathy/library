<h1>Recently Added Books</h1>
<hr />
<!-- Display the books details -->
<% recentlyBooks.map((book) => { %>
<div class="book">
  <!-- Show Book Link -->
  <a href="/books/<%= book.id %> ">
    <img src="<%= book.imgPath %>" width="100" height="150" />
    <p><%= book.title %></p>
  </a>
</div>
<% }) %>
