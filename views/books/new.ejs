<h1>Add new Book</h1>
<hr />
<form action="/books/" method="POST" class="special-form">
  <%- include('./_form-inputs.ejs') %>
  <div class="btn-group">
    <button type="submit" class="btn btn-primary">Create</button>
    <a href="/books" class="btn btn-danger">Cancel</a>
  </div>
</form>

<div class="error"><% if(error){ %> <%= error %> <% }%></div>
